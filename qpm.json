      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.3.2",
      "additionalData": {}
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
