{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "MultiplayerExtensions",
    "id": "multiplayerextensions",
    "version": "0.1.0-Dev",
    "url": "https://github.com/RedBrumbler/MultiplayerExtensions.Quest",
    "additionalData": {
      "overrideSoName": "libmpex.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm qmod build",
        "pwsh ./scripts/createqmod.ps1 MultiplayerExtensions -clean"
      ]
    }
  },
  "dependencies": [
    {
      "id": "libil2cpp",
      "versionRange": "^0.3.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.0.0",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "3402.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.16.0",
      "additionalData": {}
    },
    {
      "id": "multiplayer-core",
      "versionRange": "^1.6.0",
      "additionalData": {}
    },
    {
      "id": "paper",
      "versionRange": "^3.0.0",
      "additionalData": {}
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.0",
      "additionalData": {}
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {}
    }
  ]
}